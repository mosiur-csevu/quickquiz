<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quickquizfx.FXMLDocumentController">
   <children>
      <VBox layoutX="144.0" layoutY="133.0" prefHeight="54.0" prefWidth="42.0" spacing="20.0">
         <children>
            <Label text="ID" />
            <Label layoutX="10.0" layoutY="10.0" text="Name">
               <font>
                  <Font name="Ubuntu Light" size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="202.0" layoutY="128.0" prefHeight="72.0" prefWidth="183.0" spacing="10.0">
         <children>
            <TextField fx:id="tfID" />
            <TextField fx:id="tfName" layoutX="10.0" layoutY="10.0" />
         </children>
      </VBox>
      <Button fx:id="btnStartQuiz" layoutX="230.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleStartQuizBtn" prefHeight="27.0" prefWidth="120.0" text="START QUIZ">
         <font>
            <Font name="Ubuntu Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="197.0" layoutY="62.0" prefHeight="42.0" prefWidth="281.0" text=" QUICK QUIZ" textAlignment="CENTER">
         <font>
            <Font name="Exo 2 Black Italic" size="32.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
