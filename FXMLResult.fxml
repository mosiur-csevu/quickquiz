<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quickquizfx.FXMLResultController">
   <children>
      <VBox alignment="CENTER" layoutX="25.0" layoutY="110.0" prefHeight="90.0" prefWidth="550.0" spacing="15.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="110.0">
         <children>
            <Label prefHeight="17.0" prefWidth="148.0" text="QUICK QUIZ RESULT">
               <font>
                  <Font name="Ubuntu Bold" size="15.0" />
               </font>
            </Label>
            <Label fx:id="lblName" layoutX="10.0" layoutY="10.0" text="Name:">
               <font>
                  <Font name="Ubuntu Bold Italic" size="15.0" />
               </font>
            </Label>
            <Label fx:id="lblScore" layoutX="10.0" layoutY="27.0" text="Score:">
               <font>
                  <Font name="Ubuntu Bold Italic" size="16.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Button fx:id="btnPlayAgain" layoutX="258.0" layoutY="228.0" mnemonicParsing="false" onAction="#handlePlayAgainBtn" text="Play Again">
         <font>
            <Font name="Ubuntu Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
